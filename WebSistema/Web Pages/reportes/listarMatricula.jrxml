	<queryString>
		<![CDATA[select 
m.codigo as codigo_matricula,
fecha,nro_doc,codigo_alumno,total,
m.estado as estado_matricula,
a.codigo as codigo_alumnos,
nombre,
direccion,
email,
telefono,
celular,
sexo,
fec_nac,
a.estado as estado_alumno
from matriculas m 
inner join alumnos a on a.codigo = m.codigo_alumno
where nro_doc=$P{p_nro_doc}]]>
	</queryString>
